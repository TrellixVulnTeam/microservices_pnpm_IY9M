#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/pnpm@6.10.3/node_modules/pnpm/bin/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/pnpm@6.10.3/node_modules/pnpm/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/pnpm@6.10.3/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/node_modules:/Users/arjun-acc/node_modules:/Users/node_modules:/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/pnpm/bin/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/pnpm@6.10.3/node_modules/pnpm/bin/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/pnpm@6.10.3/node_modules/pnpm/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/pnpm@6.10.3/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/.pnpm/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/node_modules:/Users/arjun-acc/node_modules:/Users/node_modules:/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/pnpm/bin/node_modules:/Users/arjun-acc/OneDrive - Applied Cloud Computing/my_projects/pnpm/sample/node_modules/pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pnpm/bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../pnpm/bin/pnpx.cjs" "$@"
fi
